// (\x.x)a

// (\x.\y.y) a b


letrec insert = \x.\xs.
  if xs == # then
    x : #
  else if (x <= (hd xs)) then
    x : xs
  else
    (hd xs) : (insert x (tl xs))
in
letrec sort = \xs.
  if xs == # then
    #
  else
    insert (hd xs) (sort (tl xs))
in
sort (5 : 3 : 4 : #)