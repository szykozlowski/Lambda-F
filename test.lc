// (\x.x)a

// (\x.\y.y) a b

letrec f = \x. if x==0 then 1 else x * f(x-1) in f 5 // 120

;;

letrec fib = \ n. 
    if n==0 then 0 else 
        if n==1 then 1 else
            fib(n-2)+fib(n-1)
in
fib 10 // 55

;;

letrec insert = \x.\xs.
  if xs == # then
    x : #
  else if (x <= (hd xs)) then
    x : xs
  else
    (hd xs) : (insert x (tl xs))
in
letrec sort = \xs.
  if xs == # then
    #
  else
    insert (hd xs) (sort (tl xs))
in
sort (5 : 3 : 4 : 3 : 1 : #)